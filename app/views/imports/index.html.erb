<p style="color: green"><%= notice %></p>

<h1>Imports</h1>

<div id="imports">
  <% @imports.each do |import| %>
    <%= render import %>
    <p>
      <%= link_to "Show this import", import %>
    </p>
  <% end %>
</div>
<h3>Import CSV files</h3>
<%= form_tag imports_url , multipart: true do %>
<%= file_field_tag :file %>
<%= submit_tag "Upload File" %>
<% end %>
<%= link_to "New import", new_import_path %>
